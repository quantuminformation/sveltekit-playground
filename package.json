		"lint": "prettier --check .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "next",
		"@sveltejs/kit": "next",
		"svelte": "^3.29.0",
		"vite": "^2.1.0",
		"prettier": "~2.2.1",
		"prettier-plugin-svelte": "^2.2.0"
	},
	"type": "module"
}
